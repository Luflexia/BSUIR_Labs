            <Rule
                InputExts="*.f"
                OutputExts="*.o"
                LinkObject="1">
                <Exec CmdLine='gfortran -O -g -o "%bd%n.o" "%f"'/>
            </Rule>
            <Rule
                InputExts="*.f90"
                OutputExts="*.o"
                LinkObject="1">
                <Exec CmdLine='gfortran -O -g -o "%bd%n.o" "%f"'/>
            </Rule>
            <Rule
                InputExts="*.d"
                OutputExts="*.o"
                LinkObject="1">
                <Exec CmdLine='gdc -c -g -o "%bd%n.o" "%f"'/>
            </Rule>
        </Rules>
        <List Name="GNUC Options">
            <Item
                Name="LinkerOutputType"
                Value="Executable"/>
        </List>
    </Config>
    <Files>
        <Folder
            Name="Source Files"
            Filters="*.c;*.C;*.cc;*.cpp;*.cp;*.cxx;*.c++;*.prg;*.pas;*.dpr;*.asm;*.s;*.bas;*.java;*.cs;*.sc;*.scala;*.e;*.cob;*.html;*.rc;*.tcl;*.py;*.pl;*.d;*.m;*.mm;*.go;*.groovy;*.gsh"
            GUID="{3AECFC9C-CD72-43AB-A4C1-0B18EB848A2D}">
            <F N="../../src/lib.c"/>
            <F N="../../src/main.c"/>
        </Folder>
        <Folder
            Name="Header Files"
            Filters="*.h;*.H;*.hh;*.hpp;*.hxx;*.h++;*.inc;*.sh;*.cpy;*.if"
            GUID="{469AC070-31D1-4B07-93DD-11B515B16E2F}">
            <F N="../../src/lib.h"/>
        </Folder>
        <Folder
            Name="Resource Files"
            Filters="*.ico;*.cur;*.dlg"
            GUID="{16916CD7-00F0-4A7A-92C6-76A783110193}"/>
        <Folder
            Name="Bitmaps"
            Filters="*.bmp"
            GUID="{E0B22CC0-C1DB-4755-BA86-9486F6A9742C}"/>
        <Folder
            Name="Other Files"
            Filters=""
            GUID="{C994A4D6-2846-43C5-92A8-08A6082A10C5}">
            <F
                N="Makefile"
                Type="Makefile"/>
        </Folder>
    </Files>
    <List Name="RTE">
    </List>
</Project>
